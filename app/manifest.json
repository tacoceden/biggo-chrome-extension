{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "update_url": "https://github.com/tacoceden/biggo-chrome-extension",
  "icons": {
    "16": "images/biggo.png",
    "128": "images/biggo.png"
  },
  "default_locale": "zh_TW",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/biggo.png",
      "38": "images/biggo.png"
    },
    "default_title": "BigGo Extension",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["styles/bootstrap.min.css"]
}
